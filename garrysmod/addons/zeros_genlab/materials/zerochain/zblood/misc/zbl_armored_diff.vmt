"VertexLitGeneric"
{
	"$baseTexture"			"zerochain/zblood/misc/zbl_armored_diff"
	

	"$bumpMap"				"zerochain/zblood/misc/zbl_armored_nrm"

	"$surfaceprop" "watermelon"
	"$model" 1
	$halflambert 1
	
	$phong 1

	$phongexponent 2
	$phongfresnelranges "[1 1 1]"
	$phongalbedotint 1
	$phongboost 5
	$phongalbedoboost 1
	$phongtint "[1 0.4 0]"
	
	$envmap env_cubemap
	"$normalmapalphaenvmapmask" "1"	
	$envmaptint "[5 5 5]"
	$envmapfresnel 1
	$envmapFresnelMinMaxExp "[1 1 1]"

	"$nodecal" "1"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "1"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "0.5"										// Boost for ambient cube component of rim lighting


	// Emissive Scroll Pass
	"$emissiveBlendEnabled"     "1"
	"$emissiveBlendTexture" "zerochain/zblood/misc/null"
	"$emissiveBlendBaseTexture" "zerochain/zblood/misc/zbl_armored_em"
	"$emissiveBlendFlowTexture" "zerochain/zblood/misc/null"
	"$emissiveBlendTint"        "[3 3 3]"
	"$emissiveBlendStrength" "1"
	"$emissiveBlendScrollVector" "[0.1 0.12]"

	"$time" "0"
	"Proxies"
	{
		"CurrentTime"
		{
			"resultVar" "$time"
		}
	}	
}
