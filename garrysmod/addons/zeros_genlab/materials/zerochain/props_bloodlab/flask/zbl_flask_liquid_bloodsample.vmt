"VertexLitGeneric"
{
	$color2 "[1 0 0]"
	"$baseTexture"			"zerochain/zblood/wounds/zbl_wound_flesh_diff"

	"$bumpMap"				"zerochain/zblood/wounds/zbl_wound_flesh_nrm"

	"$surfaceprop" "watermelon"
	"$model" 1
	$halflambert 1

	$envmap env_cubemap
	"$normalmapalphaenvmapmask" "1"	
	$envmaptint "[0.1 0.1 0.1]"
	$envmapfresnel 0.1
	

	$phong 1
	$phongexponenttexture "zerochain/props_bloodlab/flask/zbl_flask_liquid_exp"	
	$phongfresnelranges "[1 2 3]"
	$phongalbedotint 1
	$phongboost 2
	
	$rimlight               1
	$rimlightexponent      1
	$rimlightboost         0.1
	$rimmask 1	
	
	
	
	"$translate" "[0 0]"
    "$center" "[0.5 0.5]"	
	"$def_scale" "[1 1]"
	"$angle" "0"
	
	"Proxies"
	{

		"LinearRamp"
		{
            "rate" 1
            "initialValue" 0.0
            "resultVar" "$angle"
		}
		
		"LinearRamp"
		{
            "rate" 0.01
            "initialValue" 0.0
            "resultVar" "$translate[1]"
		}
		
		"LinearRamp"
		{
            "rate" 0.01
            "initialValue" 0.0
            "resultVar" "$translate[2]"
		}
		
        "TextureTransform"
		{
            "translateVar" "$translate"
			"scaleVar"  "$def_scale"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$basetexturetransform"
		}
		
		    "TextureTransform"
		{
            "translateVar" "$translate"
			"scaleVar"  "$def_scale"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$bumptransform"
		}
	}			
}
